{
  "name": "authentication-backend",
  "version": "1.4.0",
  "private": true,
  "description": "minimal API for integration with external authentication providers",
  "author": "David Evans",
  "license": "MIT",
  "keywords": [
    "oauth"
  ],
  "exports": {
    "./backend": {
      "default": "./backend/index.mjs",
      "types": "./backend/index.d.ts"
    },
    "./jwt": {
      "default": "./jwt/index.mjs",
      "types": "./jwt/index.d.ts"
    },
    "./mock": {
      "default": "./mock/index.mjs",
      "types": "./mock/index.d.ts"
    }
  },
  "scripts": {
    "start": "package/build.sh && package/start.sh",
    "format": "prettier --write .",
    "test": "lean-test --preprocess=tsc --parallel && package/build.sh && package/run.sh && tsc && prettier --check .",
    "dopublish": "package/build.sh && npm publish package.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/auth-backend.git"
  },
  "bugs": {
    "url": "https://github.com/davidje13/auth-backend/issues"
  },
  "homepage": "https://github.com/davidje13/auth-backend#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "0.4.x",
    "@rollup/plugin-typescript": "12.x",
    "@types/node": "24.x",
    "@types/supertest": "6.x",
    "lean-test": "2.x",
    "prettier": "3.6.2",
    "rollup": "4.x",
    "rollup-plugin-dts": "6.x",
    "supertest": "7.x",
    "typescript": "5.9.x",
    "web-listener": "0.13.2"
  }
}
